{
	"layer": "top",
	"position": "top",
	"modules-left": ["custom/wofi"],
	"modules-right": ["tray","pulseaudio","network","clock","custom/wlogout"],
	"custom/wofi": {
		"format": "󰣇",
		"on-click": "wofi --show drun --allow-images",
		"tooltip": false
	},
	"tray": {
	    "icon-size": 21,
	    "spacing": 10
	},
    "pulseaudio": {
        "format": "󰕾",
        "format-muted": "󰝟",
        "tooltip": true,
        "tooltip-format": "Volume: {volume}%"
    },
    "network": {
        "format": "󰤨",
        "format-disconnected": "󰤮",
        "tooltip": true,
        "tooltip-format": "Connected to {essid}"
    },
    "clock": {
        "format": "{:%R}",
        "locale": "vi_VN.utf8",
        "tooltip": true,
        "tooltip-format": "{:%A, %d %B %Y}"
    },
    "custom/wlogout": {
        "format": "󰐥",
        "on-click": "wlogout",
        "tooltip": false
    }
}